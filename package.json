{
    "name": "haut-test-meter",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "wdio": "wdio run ./wdio.conf.ts",
        "wdio:dev": "wdio run ./wdio.conf.ts --watch true",
        "story:dev": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview",
        "deploy": "npm run build && npm run story:build && npx gh-pages --nojekyll true -d dist"
    },
    "devDependencies": {
        "@histoire/plugin-svelte": "^0.17.6",
        "@sveltejs/vite-plugin-svelte": "^3.0.1",
        "@testing-library/svelte": "^4.0.5",
        "@tsconfig/svelte": "^5.0.2",
        "@wdio/browser-runner": "^8.26.1",
        "@wdio/cli": "^8.28.6",
        "@wdio/mocha-framework": "^8.24.12",
        "@wdio/spec-reporter": "^8.24.12",
        "gh-pages": "^6.1.0",
        "histoire": "^0.17.6",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "wdio": "^6.0.1",
        "wdio-vite-service": "^1.0.6"
    }
}
